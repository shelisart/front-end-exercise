//IIFE FUNCTION
const angka = 97;

let result = (function(angka) {
    if (angka % 2 === 0) {
        return angka + " adalah bilangan genap";
    } else {
        return angka + " adalah bilangan ganjil";
    }
})(angka);
console.log(result);


